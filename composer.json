{
    "name": "myonlinestore/common-factories",
    "description": "Common Factories",
    "homepage": "https://github.com/MyOnlineStore/common-factories",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\Common\\Factory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyOnlineStore\\Common\\Factory\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "psr/http-message": "^1.0",
        "ircmaxell/random-lib": "^1.2",
        "symfony/lock": "^4.4 || ^5.2",
        "nyholm/psr7": "^1.4",
        "symfony/http-foundation": "^5.2"
    },
    "config": {
        "platform": {
            "php": "7.2.24"
        }
    },
    "require-dev": {
        "myonlinestore/coding-standard": "^3.0",
        "psalm/plugin-phpunit": "^0.15",
        "vimeo/psalm": "^4.6",
        "phpunit/phpunit": "^8.5"
    }
}
