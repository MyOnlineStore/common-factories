    "description": "Common Factories",
    "homepage": "https://github.com/MyOnlineStore/common-factories",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\Common\\Factory\\": "src/"
