        "laminas/laminas-diactoros": "^1.8",
        "ircmaxell/random-lib": "^1.2",
        "symfony/lock": "^3.4"
    },
    "config": {
        "platform": {
            "php": "7.0.33"
        }
    },
    "require-dev": {
        "myonlinestore/coding-standard": "^1.0",
        "vimeo/psalm": "^3.2",
        "phpunit/phpunit": "^6.5"
    }
