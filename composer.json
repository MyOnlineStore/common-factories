{
    "name": "myonlinestore/common-factories",
    "description": "Common Factories",
    "homepage": "https://github.com/MyOnlineStore/common-factories",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\Common\\Factory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyOnlineStore\\Common\\Factory\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "psr/http-message": "^1.0",
        "ircmaxell/random-lib": "^1.2",
        "symfony/lock": "^5.4 || ^6.0",
        "nyholm/psr7": "^1.5",
        "symfony/http-foundation": "^5.4 || ^6.0"
    },
    "require-dev": {
        "myonlinestore/coding-standard": "^4.0",
        "myonlinestore/php-devtools": "^0.3",
        "psalm/plugin-phpunit": "^0.17",
        "vimeo/psalm": "^4.23",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
